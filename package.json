{
    "name": "organizerapi",
    "version": "1.0.0",
    "description": "",
    "main": "dist/server.js",
    "scripts": {
        "clean": "rimraf ./dist",
        "dev": "set NODE_ENV=development&& set DEBUG=app && nodemon --exec ts-node ./src/server.ts",
        "prebuild": "npm run clean && tslint -c tslint.json -p tsconfig.json --fix",
        "build": "tsc -p .",
        "heroku-postbuild": "npm run build",
        "start": "npm run serve",
        "serve": "set NODE_ENV=production&& node ./dist/server",
        "watch-node": "set DEBUG=app && node ./dist/server",
        "watch-ts": "tsc -w",
        "test": "jest --passWithNoTests --detectOpenHandles --forceExit --testTimeout=10000",
        "test:migration": "typeorm migration:run --connection test",
        "prod:migration": "typeorm migration:run --connection production",
        "prod:migration:revert": "typeorm migration:revert --connection production",
        "test:github": "set NODE_ENV=github&& jest --passWithNoTests --detectOpenHandles --forceExit --testTimeout=10000",
        "migrate:github": "typeorm migration:run --connection github",
        "migration:gen": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:generate --connection test -n",
        "migration:run": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:run --connection test",
        "migration:revert": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:revert --connection test"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/CodingForFunAndProfit/OrganizerApi.git"
    },
    "author": "Florian Stolz",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/CodingForFunAndProfit/OrganizerApi/issues"
    },
    "homepage": "https://github.com/CodingForFunAndProfit/OrganizerApi#readme",
    "dependencies": {
        "apollo-server-express": "^2.11.0",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "compression": "^1.7.4",
        "connect-redis": "^4.0.4",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-session": "^1.17.0",
        "graphql": "^14.6.0",
        "graphql-query-complexity": "^0.4.1",
        "ioredis": "^4.16.0",
        "morgan": "^1.10.0",
        "nodemailer": "^6.4.6",
        "pg": "^7.18.2",
        "reflect-metadata": "^0.1.13",
        "type-graphql": "^0.17.6",
        "typeorm": "^0.2.24",
        "uuid": "^7.0.2",
        "jsonwebtoken": "^8.5.1"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/body-parser": "^1.19.0",
        "@types/compression": "^1.7.0",
        "@types/connect-redis": "0.0.13",
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "^2.8.6",
        "@types/debug": "^4.1.5",
        "@types/express": "^4.17.3",
        "@types/express-session": "^1.17.0",
        "@types/faker": "^4.1.11",
        "@types/ioredis": "^4.14.9",
        "@types/jest": "^25.1.4",
        "@types/jsonwebtoken": "^8.3.8",
        "@types/morgan": "^1.9.0",
        "@types/node": "^13.9.5",
        "@types/nodemailer": "^6.4.0",
        "@types/supertest": "^2.0.8",
        "@types/uuid": "^7.0.2",
        "cross-env": "^7.0.2",
        "faker": "^4.1.0",
        "jest": "^25.2.3",
        "nodemon": "^2.0.2",
        "rimraf": "^3.0.2",
        "supertest": "^4.0.2",
        "ts-jest": "^25.2.1",
        "ts-node": "^8.8.1",
        "tslint": "^6.1.0",
        "typescript": "3.7.5"
    }
}
